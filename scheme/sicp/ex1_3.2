(define (<= x y)
  (not (> x y)))

(define (square x) (* x x))

(define (min x y z)
  (cond ((and (<= x y) (<= x z)) x)
        ((and (<= y x) (<= y z)) y)
        (else z)))

(define (square-of-bigger x y z)
  (let ((minimal (min x y z)))
  (- (+ (square x) (square y) (square z))
     (square minimal))))
